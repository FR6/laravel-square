{
    "name": "nikolag/square",
    "description": "Square API integration with Laravel built on nikolag/core",
    "keywords": ["square", "laravel", "square laravel integration", "square-laravel", "library", "package"],
    "license": "MIT",
    "type": "laravel-package",
    "support": {
        "issues": "https://github.com/NikolaGavric94/nikolag-square/issues",
        "source": "https://github.com/NikolaGavric94/nikolag-square",
        "wiki": "https://github.com/NikolaGavric94/nikolag-square/wiki",
        "email": "nikola.gavric94@gmail.com"
    },
    "authors": [{
        "name": "Nikola Gavric",
        "email": "nikola.gavric94@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "square/connect": "2.5.*",
        "nikolag/core": "2.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.6.*"
    },
    "autoload": {
        "psr-4": {
            "Nikolag\\Square\\": "src/",
            "Nikolag\\Square\\Contracts\\": "src/contracts",
            "Nikolag\\Square\\Exceptions\\": "src/exceptions",
            "Nikolag\\Square\\Facades\\": "src/facades",
            "Nikolag\\Square\\Models\\": "src/models",
            "Nikolag\\Square\\Providers\\": "src/providers",
            "Nikolag\\Square\\Traits\\": "src/traits",
            "Nikolag\\Square\\Utils\\": "src/utils"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nikolag\\Square\\": "src/",
            "Nikolag\\Square\\Contracts\\": "src/contracts",
            "Nikolag\\Square\\Exceptions\\": "src/exceptions",
            "Nikolag\\Square\\Facades\\": "src/facades",
            "Nikolag\\Square\\Models\\": "src/models",
            "Nikolag\\Square\\Providers\\": "src/providers",
            "Nikolag\\Square\\Traits\\": "src/traits",
            "Nikolag\\Square\\Utils\\": "src/utils",
            "Nikolag\\Square\\Tests\\": "tests/",
            "Nikolag\\Square\\Tests\\Models\\": "tests/classes/"
        }
    },
    "scripts": {
        "post-package-install": [
            "php artisan vendor:publish --tag=nikolag_config"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Nikolag\\Square\\Providers\\SquareServiceProvider"
            ],
            "aliases": {
                "Square": "Nikolag\\Square\\Facades\\Square"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}